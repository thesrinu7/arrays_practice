import java.util.*;
class Evenandodd
{
	public static void main(String []args)
	{
		Scanner sc=new Scanner(System.in);
		int s=sc.nextInt();
		int arr []=new int[s];
		for(int i=0;i<s;i++)
		{
			arr[i]=sc.nextInt();
		}
		for(int i=0;i<s;i++)
		{
			if(arr[i]%2==0)
			{
				System.out.println("even nums: "+arr[i]);
			}
			else
			{
				System.out.println("odd nums: "+arr[i]);
			}
		}
	}
}
		
		
