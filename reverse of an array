import java.util.*;
class Rev
{
	public static void main(String []args)
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("enter the size:");
		int a=sc.nextInt();
		int arr[]= new int[a];
		System.out.println("enter your input elements");
		for(int i=0;i<a;i++)
		{
			arr[i]=sc.nextInt();
		}
		System.out.println("\nreverse of an array is");
		for(int i=a-1;i>=0;i--)
		{
			System.out.print(arr[i]+" ");
		}
	}
}

		
			
